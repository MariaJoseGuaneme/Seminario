[general]
bindport=5060
bindaddr=0.0.0.0
nat=force_rport,comedia
localnet=192.168.3.0/24  ; Ajusta esto seg√∫n tu red WiFi (verifica con `ip a`)
localnet=192.168.20.0/24
localnet=192.168.50.0/24
disallow=all
allow=ulaw
allow=alaw
language=es
qualify=yes
transport=udp


[authentication]
[basic-options](!)                ; a template
        dtmfmode=rfc2833
        context=from-office
        type=friend
[natted-phone](!,basic-options)   ; another template inheriting basic-options
        directmedia=no
        host=dynamic
[public-phone](!,basic-options)   ; another template inheriting basic-options
        directmedia=yes
[my-codecs](!)                    ; a template for my preferred codecs
        disallow=all
        allow=ilbc
        allow=g729
        allow=gsm
        allow=g723
        allow=ulaw
[ulaw-phone](!)                   ; and another one for ulaw-only
        disallow=all
        allow=ulaw
[usuario](!)
type=friend
host=dynamic
context=internal
disallow=all
allow=ulaw
nat=yes
directmedia=no
qualify=yes


;Extension 101
[ext101](usuario)
username=uno
secret=uno

;Extension 102
[ext102](usuario)
username=dos
secret=dos

;Extension 103
[ext103](usuario)
username=tres
secret=tres

;Extension 104
[ext104](usuario)
username=cuatro
secret=cuatro

;Extension 105
[ext105](usuario)
username=cinco
secret=cinco

;Extension 106
[ext106](usuario)
username=seis
secret=seis

